{
	"name": "rippin",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "bunx --bun tsc -b && vite build",
		"preview": "bunx --bun vite preview",
		"fix": "bunx --bun biome check --write ./",
