	},
	"dependencies": {
		"@tailwindcss/postcss": "^4.1.11",
		"clsx": "^2.1.1",
		"decimal.js": "^10.5.0",
		"postcss": "^8.5.6",
		"preact": "^10.26.9",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@preact/preset-vite": "^2.10.2",
		"typescript": "~5.8.3",
		"vite": "^7.0.0"
	}
}
